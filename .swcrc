{
  "$schema": "https://json.schemastore.org/swcrc",
  "exclude": "node_modules/",
  "sourceMaps": false,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "useDefineForClassFields": true,
      "topLevelAwait": true
    },
    "transform": {
      "decoratorVersion": "2022-03"
    },
    "keepClassNames": true,
    "target": "esnext",
    "baseUrl": "./"
  },
  "module": {
    "type": "es6"
  },
  "minify": false
}
